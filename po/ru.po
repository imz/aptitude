msgid " -u      : Download new package lists on startup.\n"
msgstr " -u      : Загрузить список новых пакетов при запуске.\n"

#: src/main.cc:164
#, c-format
msgid " -i      : Perform an install run on startup.\n"
msgstr " -i      : Запустить установку при запуске.\n"

