#: src/cmdline/cmdline_resolver.cc:176
msgid "examine the solution in the visual user interface"
msgstr "ཐབས་ཤེས་དེ་མཐོང་བའི་ལག་ལེན་པ་ངོས་འདྲ་བའི་ནང་ལུ་བརྟག་དཔྱད་འབད།"

#: src/cmdline/cmdline_resolver.cc:178
msgid "reject the given package versions; don't display any solutions in which they occur."
msgstr "བཀོད་དེ་ཡོད་མི་ཐུམ་སྒྲིལ་ཐོན་རིམ་དེ་ཚུ་གི་དང་ལེན་སྤང་། དེ་ཚུ་བྱུང་སའི་ནང་ལུ་ ཐབས་ཤེས་ཚུ་ག་ནི་ཡང་བཀྲམ་སྟོན་མ་འབད།"

#: src/cmdline/cmdline_resolver.cc:180
msgid "accept the given package versions; display only solutions in which they occur."
msgstr "བཀོད་དེ་ཡོད་མི་ཐུམ་སྒྲིལ་ཐོན་རིམ་ཚུ་དང་ལེན་འབད། དེ་ཚུ་བྱུང་སའི་ནང་ལུ་ཐབས་ཤེས་རྐྱངམ་ཅིག་བཀྲམ་སྟོན་འབད།"

#: src/cmdline/cmdline_resolver.cc:182
