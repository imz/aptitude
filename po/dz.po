#: src/ui.cc:1960
msgid "Waste time trying to find mines"
msgstr "འགས་རྫས་ཚུ་འཚོལ་ནིའི་འབད་རྩོལ་བསྐྱེད་དགོ་པ་ཅིན་དུས་་ཚོད་འབྱིན་དགོཔ་ཨིན།"

#: src/ui.cc:1446
#: src/ui.cc:1506
msgid "Cleaning while a download is in progress is not allowed"
msgstr "ཕབ་ལེན་ཡར་འཕེལ་འབད་བའི་སྒང་ཡོད་མི་དེ་མ་ཆོགཔ་ད་ལུ་གཙང་དག་བཟོ་དོ།"

#: src/ui.cc:1449
msgid "Deleting downloaded files"
msgstr "ཕབ་ལེན་འབད་ཡོད་པའི་ཡིག་སྣོད་ཚུ་བཏོན་གཏང་དོ།"

#: src/ui.cc:1463
msgid "Downloaded package files have been deleted"
msgstr "ཕབ་ལེན་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཡིག་སྣོད་ཚུ་བཏོན་གཏང་ནུག"

#: src/ui.cc:1503
msgid "The apt cache file is not available; cannot auto-clean."
msgstr "ཨེཔཊི་འདྲ་མཛོད་ཀྱི་ཡིག་སྣོད་དེ་འཐོབ་མི་ཚུགས། རང་བཞིན་གྱིས་གཙང་དག་བཟོ་མི་ཚུགས།"

#: src/ui.cc:1529
#, c-format
msgid "Obsolete downloaded package files have been deleted, freeing %sB of disk space."
msgstr "ཕན་མེད་ཀྱི་ཕབ་ལེན་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཡིག་སྣོད་ཚུ་བཏོན་གཏང་ནུག ཌིཀསི་བར་སྟོང་གི་ %sB དེ་རང་དབང་ཅན་བཟོ་དོ།"

#: src/ui.cc:1626
msgid "No more solutions."
msgstr "ཐབས་ཤེས་དེ་ལས་ལྷག་སྟེ་མེད།"

#: src/ui.cc:1814
msgid "Unable to find a solution to apply."
msgstr "འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལུ་ཐབས་ཤེས་ཅིག་འཚོལ་མ་འཐོབ།"

#: src/ui.cc:1820
msgid "Ran out of time while trying to find a solution."
msgstr "ཐབས་ཤེས་ཅིག་འཚོལ་བའི་སྐབས་ལུ་དུས་ཚོད་ཀྱི་བདའ་མ་ཟུན།"

#: src/ui.cc:1863
#: src/ui.cc:1865
msgid "Resolve Dependencies"
msgstr "རྟེན་འབྲེལ་ཚུ་མོས་མཐུན་བཟོ།"

#: src/ui.cc:1864
msgid "Search for solutions to unsatisfied dependencies"
msgstr "ངལ་རངས་མ་འབྱུང་པའི་རྟེན་འབྲེལ་ཚུ་གི་དོན་ལུ་ ཐབས་ཤེས་ཚུ་འཚོལ་ཞིབ་འབད།"

#: src/ui.cc:1876
#, c-format
msgid "Unable to open %ls"
msgstr "%ls དེ་ཁ་ཕྱེ་མི་ཚུགས་པས།"

#: src/ui.cc:1882
msgid "Error while dumping resolver state"
msgstr "མོས་མཐུན་བཟོ་མི་གནས་ལུགས་དེ་བཀོག་བཞག་པའི་སྐབས་ལུ་འཛོལ་བ་བྱུང་ནུག"

#: src/ui.cc:1892
msgid "File to which the resolver state should be dumped:"
msgstr "མོས་མཐུན་བཟོ་མི་གནས་ལུགས་དེ་བཀོག་བཞག་དགོ་སའི་ཡིག་སྣོད:"

#: src/ui.cc:1922
msgid "^Install/remove packages"
msgstr "ཐུམ་སྒྲིལ་ཚུ་གཞི་བཙུགས/རྩ་བསྐྲད་གཏང་།(^I)"

#: src/ui.cc:1923
msgid "Perform all pending installs and removals"
msgstr "བསྣར་ཏེ་ཡོད་པའི་གཞི་བཙུགས་འབད་ནི་དང་རྩ་བསྐྲད་གཏང་ནི་ཚུ་ཆ་མཉམ་ལག་ལེན་འཐབ།"

#: src/ui.cc:1925
msgid "^Update package list"
msgstr "ཐུམ་སྒྲིལ་གྱི་ཐོ་ཡིག་དེ་དུས་མཐུན་བཟོ།(^U)"

#: src/ui.cc:1926
msgid "Check for new versions of packages"
msgstr "ཐུམ་སྒྲིལ་ཚུ་གི་ཐོན་རིམ་གསརཔ་ཚུ་གི་དོན་ལུ་ཞིབ་དཔྱད་འབད།"

#: src/ui.cc:1930
msgid "Mark ^Upgradable"
msgstr "ཡར་བསྐྱེད་འབད་བཏུབ་མི་ལུ་རྟགས་བཀལ།(^U)"

#: src/ui.cc:1931
msgid "Mark all upgradable packages which are not held for upgrade"
msgstr "ཡར་བསྐྱེད་འབད་བཏུབ་མི་ཐུམ་སྒྲིལ་ཡར་བསྐྱེད་འབད་ནིའི་དོན་ལུ་འཆང་སྟེ་མེད་མི་མི་ཚུ་ཆ་མཉམ་ལུ་རྟགས་བཀལ།"

#: src/ui.cc:1935
msgid "^Forget new packages"
msgstr "ཐུམ་སྒྲིལ་གསརཔ་ཚུ་སེམས་ཁར་ལས་བརྗེད་ད།(^F)"

#: src/ui.cc:1936
msgid "Forget which packages are \"new\""
msgstr "ཐུམ་སྒྲིལ་ག་དེ་\"new\"ཨིན་ན་སེམས་ཁར་ལས་བརྗེད་ད།"

#: src/ui.cc:1939
msgid "Canc^el pending actions"
msgstr "བསྣར་ཏེ་ཡོད་པའི་བྱ་བ་ཚུ་ཆ་མེད་གཏང་།(^e)"

#: src/ui.cc:1940
msgid "Cancel all pending installations, removals, holds, and upgrades."
msgstr "བསྣར་ཏེ་ཡོད་པའི་གཞི་བཙུགས་འབད་ནི་དང་ རྩབསྐྲད་གཏང་ནི་ འཆང་ནི་དང་ཡར་བསྐྱེད་འབད་ནི་ཚུ་ཆ་མཉམ་ཆ་མེད་གཏང་།"

#: src/ui.cc:1943
msgid "^Clean package cache"
msgstr "ཐུམ་སྒྲིལ་འདྲ་མཛོད་དེ་གཙང་དག་བཟོ། (^C)"

#: src/ui.cc:1944
msgid "Delete package files which were previously downloaded"
msgstr "ཧེ་མ་ལས་ཕབ་ལེན་འབད་ཡོའ་པའི་ཐུམ་སྒྲིལ་ཡིག་སྣོད་ཚུ་བཏོན་གཏང་།"

#: src/ui.cc:1947
msgid "Clean ^obsolete files"
msgstr "ཕན་ཡིག་སྣོད་ཚུ་གཙང་དག་བཟོ། (^o)"

#: src/ui.cc:1948
msgid "Delete package files which can no longer be downloaded"
msgstr "ད་ལས་ཕར་ཕབ་ལེན་འབད་མ་བཏུབ་མི་ཐུམ་སྒྲིལ་ཡིག་སྣོད་ཚུ་བཏོན་གཏང་།"

#: src/ui.cc:1954
msgid "^Reload package cache"
msgstr "ཐུམ་སྒྲིལ་འདྲ་མཛོད་དེ་ཡང་བསྐྱར་མངོན་གསལ་འབད།(^R)"

#: src/ui.cc:1955
msgid "Reload the package cache"
msgstr "ཐུམ་སྒྲིལ་འདྲ་མཛོད་དེ་ཡང་བསྐྱར་མངོན་གསལ་འབད།"

#: src/ui.cc:1959
msgid "^Play Minesweeper"
msgstr "འགས་རྫས་འཕྱག་མི་གཏང་། (^P)"

#: src/ui.cc:1964
msgid "^Become root"
msgstr "རྩ་བ་ལུ་འགྱུར་འོང་། (^B)"

#: src/ui.cc:1965
msgid "Run 'su' to become root; this will restart the program, but your settings will be preserved"
msgstr " 'ཨེསི་ཡུ་' དེ་རྩ་བ་ལུ་འགྱུར་ནིའི་དོན་ལུ་གཡོག་བཀོལ། འདི་གིས་ལས་རིམ་དེ་སླར་འགོ་བཙུགསཔ་ཨིན་ དེ་འབདཝ་ད་ ཁྱོད་ཀྱི་སྒྲིག་སྟངས་དེ་ཉམས་སྲུང་འབད་བཞག་འོང་།"

#: src/ui.cc:1967
msgid "^Quit"
msgstr "སྤང་།(^Q)"

#: src/ui.cc:1968
msgid "Exit the program"
msgstr "ལས་རིམ་དེ་ཕྱིར་འཐོན་འབད།"

#: src/ui.cc:1974
#: src/ui.cc:2332
msgid "Undo"
msgstr "འབད་བཤོལ།"

#: src/ui.cc:1975
msgid "Undo the last package operation or group of operations"
msgstr "མཇུག་གི་ཐུམ་སྒྲིལ་བཀོལ་སྤྱོད་ ཡང་ན་ བཀོལ་སྤྱོད་སྡེ་ཚན་ཚུ་འབད་བཤོལ་བཞག"

#: src/ui.cc:1983
msgid "^Install"
msgstr "གཞི་བཙུགས།(^I)"

#: src/ui.cc:1984
msgid "Flag the currently selected package for installation or upgrade"
msgstr "གཞི་བཙུགས་དང་ཡར་བསྐྱེད་འབད་ནིའི་དོན་ལུ་ ད་ལྟོ་སེལ་འཐུ་འབད་ཡོའ་པའི་ཐུམ་སྒྲིལ་དེ་ལུ་ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:1987
msgid "^Remove"
msgstr "རྩ་བསྐྲད། (^R)"

#: src/ui.cc:1988
msgid "Flag the currently selected package for removal"
msgstr "རྩ་བསྐྲད་གཏང་ནིའི་དོན་ལུ་ ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་དེ་ལུ་ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:1991
msgid "^Purge"
msgstr "ཕྱིར་བསྐྲད། (^P)"

#: src/ui.cc:1992
msgid "Flag the currently selected package and its configuration files for removal"
msgstr "རྩ་བསྐྲད་གཏང་ནིའི་དོན་ལུ་ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་མི་ཐུམ་སྒྲིལ་དང་ དེའི་རིམ་སྒྲིག་ཡིག་སྣོད་དེ་ལུ་ ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:1995
msgid "^Keep"
msgstr "བཞག(^K)"

#: src/ui.cc:1996
msgid "Cancel any action on the selected package"
msgstr "བྱ་བ་ག་འབད་རུང་ཅིག་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་གུར་ཆ་མེད་གཏང་།"

#: src/ui.cc:1999
msgid "^Hold"
msgstr "འཆང། (^H)"

#: src/ui.cc:2000
msgid "Cancel any action on the selected package, and protect it from future upgrades"
msgstr "བྱ་བ་ག་འབད་རུང་ཅིག་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་གུར་ཆ་མེད་གཏང་ཞིནམ་ལས་ འ་ནི་དེ་མ་འོངས་པ་ལུ་ཡར་བསྐྱེད་འབད་ནི་ལས་ཉེན་སྐྱོབ་འབད།"

#: src/ui.cc:2003
msgid "Mark ^Auto"
msgstr "རང་བཞིན་ལུ་རྟགས་བཀལ། (^A)"

#: src/ui.cc:2004
msgid "Mark the selected package as having been automatically installed; it will automatically be removed if no other packages depend on it"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཚུ་རང་བཞིན་གྱིས་ཐོག་ལས་གཞི་བཙུགས་འབད་ཡོདཔ་བཟུམ་སྦེ་རྟགས་བཀལ། ཐུམ་སྒྲིལ་གཞན་མི་ཚུ་གིས་དེ་ལུ་མ་བརྟེན་པ་ཅིན་ འ་ནི་དེ་རང་བཞིན་གྱིས་རྩ་བསྐྲད་གཏང་འོང་།"

#: src/ui.cc:2007
msgid "Mark ^Manual"
msgstr "ལག་དེབ་ལུ་རྟགས་བཀལ།(^M)"

#: src/ui.cc:2008
msgid "Mark the selected package as having been manually installed; it will not be removed unless you manually remove it"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཚུ་ལག་ཐོག་ལས་གཞི་བཙུགས་འབད་དེ་ཡོད་དོ་བཟུམ་སྦེ་རྟགས་བཀལ། ཁྱོད་ཀྱིས་ལག་ཐོག་ལས་རྩ་བསྐྲད་མ་གཏང་ཚུན་ཚོད་ དེ་གིས་རྩ་བསྐྲད་མི་འགྱོ།"

#: src/ui.cc:2011
msgid "^Forbid Version"
msgstr "ཐོན་རིམ་མ་ཆོགཔ་བཟོ།(^F)"

#: src/ui.cc:2012
msgid "Forbid the candidate version of the selected package from being installed; newer versions of the package will be installed as usual"
