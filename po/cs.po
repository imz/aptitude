#: src/main.cc:120
#, c-format
msgid " install      - Install/upgrade packages\n"
msgstr " install      - Instaluje/aktualizuje balíky\n"

#: src/main.cc:121
#, c-format
msgid " remove       - Remove packages\n"
msgstr " remove       - Odstraní balíky\n"

