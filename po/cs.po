#, c-format
msgid ""
" --with(out)-recommends\tSpecify whether or not to treat recommends as\n"
"                strong dependencies\n"
msgstr ""
" --with(out)-recommends\tNastaví, zda se mají doporučené závislosti\n"
"                brát jako silné závislosti\n"

