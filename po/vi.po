msgstr "Gặp lỗi nội bộ : giải pháp vô giá trị bất thường."

#: src/broken_indicator.cc:330
#, c-format
msgid "%d install"
msgid_plural "%d installs"
msgstr[0] "%d lần cài"

#: src/broken_indicator.cc:336
