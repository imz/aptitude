#, c-format
msgid "Del %s* %spartial/*\n"
msgstr "Slet %s* %sdelvis/*\n"

#: src/cmdline/cmdline_clean.cc:80
#, c-format
msgid "Del %s %s [%sB]\n"
msgstr "Slet %s %s [%sB]\n"

#: src/cmdline/cmdline_clean.cc:136
#, c-format
