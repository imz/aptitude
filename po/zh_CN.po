#: src/main.cc:120
#, c-format
msgid " install      - Install/upgrade packages\n"
msgstr " install      - 安装/升级软件包\n"

#: src/main.cc:121
#, c-format
msgid " remove       - Remove packages\n"
msgstr " remove       - 卸载软件包\n"

